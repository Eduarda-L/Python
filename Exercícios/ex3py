#tabuada

valor = int(input("informe um numero: "))
aux = 0

print("*" * 18)
print("tabuada de {}".format(valor))
print("*" * 18)

while(aux <= 10):
    print("{0} x {1} = {2}".format(valor, aux, valor * aux))
    aux = aux + 1

#tabuada2

digito = int(input("Digite um nÃºmero entre 1 e 9: "))
i = 1
while i < 11:
    resultado = digito * i
    print("{} x {} = {}".format(digito, i, resultado))
    i += 1

